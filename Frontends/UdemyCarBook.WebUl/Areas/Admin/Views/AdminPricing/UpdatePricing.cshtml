@model UpdatePricingDto

@{
    ViewData["Title"] = "UpdatePricing";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<div class="page-content">
	<div class="container-fluid">
		<div class="row">
			<div class="col-xl-12">
				<div class="card">
					<div class="card-body">

						<h4 class="card-title"> Ödeme Periyodu Güncelleme</h4>
						<p class="card-subtitle mb-4">Ödeme alanına veri güncellemeyi aşağaıdaki panelden yapabilirsiniz. </p>

						<form method="post">
							<input type="hidden" asp-for="PricingID" />
							<div class="form-group">
								<label for="exampleInputName">Ödeme Periyot Başlığı</label>
								<input type="text" class="form-control" id="exampleInputName" asp-for="Name" placeholder="Ödeme Periyot Adını Giriniz">
							</div>


							<button type="submit" class="btn btn-success waves-effect waves-light">Ödeme Periyot Bilgisini Güncelle</button>
						</form>

					</div>
				</div>
			</div>
		</div>

	</div>
</div>




